{
    "manifest_version": 3,
    "name": "BlockIT",
    "version": "1.1.8",
    "description": "Blocks specified websites and redirects to a custom page",
    "permissions": [
      "storage",
      "webNavigation",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
  }
